    "from collections import Counter\n",
    "\n",
    "def modes(data):\n",
    "    cnts = Counter(data)\n",
    "    mx, mn = max(cnts.values()), min(cnts.values())\n",
    "    return sorted([k for k in cnts if cnts[k] == mx and cnts[k] != mn])"
   ]
  }
